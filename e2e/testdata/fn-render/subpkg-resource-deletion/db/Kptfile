apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: db
pipeline:
  mutators:
    - image: gcr.io/kpt-fn/starlark:v0.1.0
      configPath: statefulset-filter.yaml
    - image: gcr.io/kpt-fn/set-namespace:v0.1.3
      configMap:
        namespace: db
    - image: gcr.io/kpt-fn/set-labels:v0.1.4
      configMap:
        app: backend
