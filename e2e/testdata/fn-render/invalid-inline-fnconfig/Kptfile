apiVersion: kpt.dev/v1
kind: Kptfile
metadata:
  name: app
pipeline:
  mutators:
    - image: gcr.io/kpt-fn/set-namespace:v0.1.3
      configMap:
        namespace: staging
    - image: gcr.io/kpt-fn/set-labels:v0.1.4
      config: # invalid inline config, missing apiVersion/kind
        envs:
          - dev
          - staging
          - production
        tier: backend
